* {
	margin: 0;
	padding: 0;
	box-sizing: border-box;
}

html {
	background-color: #068d3d;
	color: rgb(129, 129, 129);
	font-family: 'Titillium Web';
	font-size: 20px;
	font-weight: 300;
	text-rendering: optimizeLegibility;
	scroll-behavior: smooth;
}

header {
	color: rgb(196, 108, 9);
	background-image: linear-gradient(
			rgba(46, 46, 46, 0.616),
			rgba(46, 46, 46, 0.616)
		),
		url('20200523_095811.HASH_REF_52103f104b9ab71e0965b40d13d81845.jpg');
	background-size: cover;
	background-position: unset;
	background-attachment: fixed;
	height: 100vh;
	text-align: center;
}

header h1 {
	font-size: 70px;
	margin-top: 15%;
	text-align: center;
	text-shadow: 4px 4px rgba(4, 49, 0, 0.719);
}

h2 {
	font-size: 40px;
}

h3 {
	font-size: 30px;
}

/* ****************** */

/* ****** NAVI *****  */

/* ****************** */

.main-nav {
	display: inline-block;
	text-transform: uppercase;
	font-weight: 400;
}

.nav-links {
	margin-top: 67px;
	justify-content: space-between;
}

.li-blog,
.li-about {
	margin-right: 200px;
}

.li-photos,
.li-join {
	margin-left: 200px;
}

.nav-center {
	position: absolute;
	left: 50%;
	top: 10%;
	transform: translate(-50%, -50%);
}

.nav-center img {
	width: 75px;
	border-radius: 50%;
}

.main-nav a:link,
.main-nav a:visited {
	color: rgb(253, 138, 6);
	text-decoration: none;
}

.main-nav a:hover,
.main-nav a:active {
	color: #068d3d;
	border-bottom: 2px solid;
	transition: bottom-border 0.8s, color 0.3s;
}

/* ****************** */

/* ***** MOBILE ****  */

/* ****************** */

.mobile-nav {
	display: none;
}

.mobile-links {
	display: none;
}

.mobile-nav a:link,
.mobile-nav a:visited {
	color: rgb(253, 138, 6);
	text-decoration: none;
}

.mobile-nav a:hover,
.mobile-nav a:active {
	color: #068d3d;
	border-bottom: 2px solid;
	transition: bottom-border 0.8s, color 0.3s;
}

/* ****************** */

/* ****** BODY *****  */

/* ****************** */

.container {
	display: grid;
	height: 250vh;
	grid-template-columns: 1fr 2fr 1fr;
	grid-template-rows: 2fr 1fr 1fr 1fr 1fr;
	grid-template-areas:
		'about about about-img'
		'post1-img post1 post1'
		'post2 post2 post2-img'
		'post3-img post3 post3'
		'join join join1-img';
	grid-gap: 10px;
	color: #ddd;
	padding: 100px 150px 0 150px;
	max-width: 100em;
}

/* ABOUT */

.about {
	grid-area: about;
}

.about-img {
	grid-area: about-img;
}

.about-img img {
	margin-left: 30px;
	border-radius: 5%;
	background-image: url(20200227_124348.HASH_REF_ebd234cd4b876178f56c8ac03f851577.jpg);
}

/* POSTS */

.post1 {
	grid-area: post1;
}

.post2 {
	grid-area: post2;
}

.post3 {
	grid-area: post3;
}

.post1-img {
	margin-right: 30px;
	grid-area: post1-img;
}

.post1-img img {
	margin-left: 20px;
	border-radius: 50%;
}

.post2-img {
	grid-area: post2-img;
}

.post2-img img {
	margin-left: 5px;
	border-radius: 25%;
}

.post3-img {
	grid-area: post3-img;
}

.post3-img img {
	margin-left: 80px;
	border-radius: 10%;
}

/* FORM */

.join {
	background-color: beige;
	color: #333;
	padding-left: 50px;
	margin-bottom: 10px;
	border: 2px solid #333;
	border-radius: 4px 4px;
	grid-area: join;
}

.join1-img {
	grid-area: join1-img;
}

.join1-img img {
	border-radius: 10px;
}

.join input {
	margin-top: 30px;
	font-size: 20px;
}

.btn {
	padding: 3px;
	margin-top: 20px;
	float: left;
	background-color: chocolate;
	border: none;
	border-radius: 10px;
	width: 100px;
	height: 40px;
}

.btn:hover {
	background-color: rgb(253, 138, 6);
	color: white;
	border: 2px solid rgb(0, 132, 255);
	cursor: pointer;
}

/* ****************** */

/* **** FOOTER *****  */

/* ****************** */

footer {
	display: flex;
	justify-content: center;
	flex-direction: row;
	justify-content: center;
	position: absolute;
	width: 100%;
	background: #333;
	padding: 50px;
	font-size: 80%;
}

.foot a {
	color: rgb(129, 129, 129);
}

.footer-nav {
	float: left;
	margin-left: -15rem;
}

.footer-nav a {
	padding: 20px;
	font-size: 125%;
	color: rgb(129, 129, 129);
}

.social-icons {
	float: right;
	margin-right: -12rem;
	font-size: 200%;
}

.social-icons a {
	padding: 20px;

	transition: color 0.2s;
}

.footer-nav a:visited,
.footer-nav a:link,
.ion-social-instagram-outline a:link,
.ion-social-instagram-outline a:visited,
.ion-social-youtube-outline a:link,
.ion-social-youtube-outline a:visited {
	transition: color 0.5s;
}

.footer-nav a:hover {
	color: #068d3d;
}

.ion-social-instagram:hover {
	color: #e95950;
}

.ion-social-youtube:hover {
	color: #e62117;
}

.ion-social-facebook:hover {
	color: #3b5999;
}

/* ****************** */

/* **** QUERIES ****  */

/* ****************** */

